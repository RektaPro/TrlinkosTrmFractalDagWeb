[run]
source = .
omit =
    tests/*
    venv/*
    .venv/*
    */site-packages/*
    setup.py
    run_all_tests.py
    download_data.py
    google_scraper.py
branch = True

[report]
precision = 2
show_missing = True
skip_covered = False
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @(abc\.)?abstractmethod

[html]
directory = htmlcov

[xml]
output = coverage.xml
